properties([
	parameters([
		text(name: 'MY_Text',defaultValue: '请输入',description: '请输入多好')
	])
])
pipeline {
	agent any
	stages {
		stage('Write Text to File') {
			steps {
				writeFile file: 'user_input.txt',text: "${params.MY_TEXT}"
				echo ""
			}
		}
	}
}
