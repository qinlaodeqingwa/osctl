//properties([
//	parameters([
//		password(name: 'MY_PASSWORD',defaultValue: '',description:'请输入密码')
//	])
//])
properties([
	parameter([
		credentials(
			name: 'MY_CREDENTIAL',
			credentialType: 'Username with password',
			description: '请选择凭据'
		)
	])
])
pipeline {
	agent any
	stages {
		stage('Print Password') {
			steps {
				echo "Password 参数值为: ${params.MY_PASSWORD}"
			}
		}
	}
}